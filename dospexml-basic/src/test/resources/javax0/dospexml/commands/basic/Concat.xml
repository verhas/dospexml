<Script xmlns="dospex:basic" xmlns:t="test">
    <t:DisplayName>Concat strings together</t:DisplayName>
    <t:Expected><![CDATA[The big brown box
            jumped over
            1
            lazy oven
]]></t:Expected>
    <Puts trim="true" trail="\n">
        <Concat>
            <String>The big brown box</String>
            jumped over
            <Long>1</Long>
            lazy oven
        </Concat>
    </Puts>
</Script>