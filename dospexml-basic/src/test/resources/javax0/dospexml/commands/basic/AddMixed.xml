<Script xmlns="dospex:basic" xmlns:t="test">
    <t:DisplayName>'Add' can add two or more mixed type numbers together</t:DisplayName>
    <t:Expected>4.0</t:Expected>
    <Puts trim="true">
        <Add>
            <BigDecimal>1</BigDecimal>
            <Integer>1</Integer>
            <Long>1</Long>
            <Double>1</Double>
        </Add>
    </Puts>
    <t:Expected>3.0</t:Expected>
    <Puts trim="true">
        <Add>
            <Integer>1</Integer>
            <Long>1</Long>
            <Double>1</Double>
        </Add>
    </Puts>
    <t:Expected>2</t:Expected>
    <Puts trim="true">
        <Add>
            <Integer>1</Integer>
            <Long>1</Long>
        </Add>
    </Puts>
    <t:Throws>
        <Add>
            <String>1</String>
            <Long>1</Long>
        </Add>
    </t:Throws>
    <t:Throws>
        <Add>
            <String>1</String>
            <Long>1</Long>
        </Add>
    </t:Throws>
</Script>